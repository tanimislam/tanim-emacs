(defvar mercury-mode-syntax-table
  (let ((mercury-mode-syntax-table (make-syntax-table)))
    ;; This is added so entity names with underscores can be more easily parsed
    (modify-syntax-entry ?_ "w" mercury-mode-syntax-table)
    ;; Mercury comment styles, same as Bash
    (modify-syntax-entry ?# "< b" mercury-mode-syntax-table)
    (modify-syntax-entry ?\n "> b" mercury-mode-syntax-table)
    mercury-mode-syntax-table)
  "Syntax table for mercury-mode")
        
(defun mercury-mode ()
  (interactive)
  (kill-all-local-variables)
  (use-local-map mercury-mode-map)
  (set-syntax-table mercury-mode-syntax-table)
  ;; Set up font-lock
  (set (make-local-variable 'font-lock-defaults) '(mercury-font-lock-keywords))
  ;; Register our indentation function
  (set (make-local-variable 'indent-line-function) 'mercury-indent-line)  
  (setq major-mode 'mercury-mode)
  (setq mode-name "Mercury")
  (run-hooks 'mercury-mode-hook))
        
